body {
	background-color: #F2F2F2;
	padding-top: 60px;

}

.jumbotron {
	position: relative;
	margin-bottom: 20px;
	padding: 30px 0;
	color: #FFFFFF;
	text-shadow: 0 1px 3px rgba(0,0,0,.4), 0 0 30px rgba(0,0,0,.075);
	border-bottom: 1px solid #AAAAAA;
	background: #FF2301; /* Old browsers */
	background: -moz-linear-gradient(45deg,  #FF2301 0%, #DC2301 100%); /* FF3.6+ */
	background: -webkit-gradient(linear, left bottom, right top, color-stop(0%,#FF2301), color-stop(100%,#DC2301)); /* Chrome,Safari4+ */
	background: -webkit-linear-gradient(45deg,  #FF2301 0%,#DC2301 100%); /* Chrome10+,Safari5.1+ */
	background: -o-linear-gradient(45deg,  #FF2301 0%,#DC2301 100%); /* Opera 11.10+ */
	background: -ms-linear-gradient(45deg,  #FF2301 0%,#DC2301 100%); /* IE10+ */
	background: linear-gradient(45deg,  #FF2301 0%,#992301 100%); /* W3C */
	filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#FF2301', endColorstr='#DC2301',GradientType=1 ); /* IE6-9 fallback on horizontal gradient */
	-webkit-box-shadow: inset 0 3px 7px rgba(0,0,0,.2), inset 0 -3px 7px rgba(0,0,0,.2);
	-moz-box-shadow: inset 0 3px 7px rgba(0,0,0,.2), inset 0 -3px 7px rgba(0,0,0,.2);
	box-shadow: inset 0 3px 7px rgba(0,0,0,.2), inset 0 -3px 7px rgba(0,0,0,.2);
}

.jumbotron h1 {
	font-size: 60px;
	font-weight: bold;
	letter-spacing: -1px;
	line-height: 1;
}

.jumbotron p {
	font-size: 24px;
	line-height: 1.25;
}

.jumbotron .container {
	position: relative;
	z-index: 2;
}

.jumbotron:after {
	content: '';
	display: block;
	position: absolute;
	top: 0;
	right: 0;
	bottom: 0;
	left: 0;
	background: url(../img/bg-pattern.png) repeat center center;
	opacity: .1;
}

.box {
	margin-bottom: 20px;
	background-color: #FEFEFE;
	border: 1px solid #DDDDDD;
}

.padded {
	padding: 10px;
}

.text-center { text-align: center; }
.text-right { text-align: right; }

.alert ul {
	margin: 5px 10px;
}